{
    "command": "/usr/local/bin/container-scripts/bootstrap.sh",
    "config_files": [
        {
            "source": "/var/lib/config-data/merged/octavia.conf",
            "dest": "/etc/octavia/octavia.conf",
            "owner": "octavia",
            "perm": "0600"
        }
    ],
    "permissions": [
        {
            "path": "/var/log/octavia",
            "owner": "octavia:octavia",
            "recurse": true
        },
        {
            "path": "/run/octavia",
            "owner": "octavia:octavia",
            "recurse": true
        }
    ]
}